{
    "name": "Enews",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Every news around the globe on your Single click in your PC.",
    "author": "Siddharth Gautam",
    "action": {
        "default_popup": "index.html",
        "default_icon": "bhagavad-gita.png",
        "default_title": "Breaking news on Hot Topics."
    },
    "host_permissions": [
        "https://newsapi.org/v2/*"
    ],
    "icons": {
        "128": "bhagavad-gita.png"
    },
    "permissions": [
        "activeTab", "<all_urls>"
    ]
}